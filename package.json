{
  "name": "@agendaedu/ae-web-components",
  "description": "Agenda Edu React Components Library",
  "version": "3.0.3",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/ae-web-components.esm.js",
  "jsnext:main": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "repository": "git@github.com:agendaedu/ae-web-components.git",
  "scripts": {
    "start": "yarn storybook",
    "build": "tsdx build",
    "test": "jest --passWithNoTests",
    "lint": "eslint src -c .eslintrc.js --ext \"ts,tsx\"",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "android": "react-native run-android",
    "clean:tags": "git tag -l | xargs git tag -d && git fetch --tags",
    "generate": "plop",
    "prebuild": "rimraf dist",
    "prepare:version": "scripts/./prepare_version.sh",
    "pretest": "jest --clearCache",
    "prettify": "prettier --check src --write",
    "send:codecov:report": "scripts/./send_codecov_report.sh",
    "set:git:hooks": "scripts/./set_git_hooks.sh",
    "sync:current:branch": "cross-env scripts/./sync_current_branch.sh",
    "test:changed": "jest -o",
    "test:ci": "jest --ci --maxWorkers=2 --collectCoverage",
    "test:coverage": "jest --coverage --colors",
    "test:staged": "jest --ci --findRelatedTests",
    "test:watch": "jest --watch",
    "version:if_main_branch": "cross-env scripts/./version_if_main_branch.sh"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "yarn prettify",
      "yarn lint --fix",
      "yarn test:staged"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "bundledDependencies": [
    "styled-system",
    "@types/styled-system",
    "styled-components",
    "polished",
    "linkifyjs",
    "linkify-react",
    "flatlist-react",
    "yup",
    "react-hook-form",
    "@hookform/resolver",
    "react-modal",
    "reactjs-popup",
    "@emoji-mart/data",
    "@emoji-mart/react"
  ],
  "peerDependencies": {
    "@emoji-mart/data": "^1.0.8",
    "@emoji-mart/react": "^1.0.1",
    "@hookform/resolver": "^2.9.10",
    "flatlist-react": "^1.5.6",
    "linkify-react": "^4.0.2",
    "linkifyjs": "^4.0.2",
    "react": ">=16",
    "react-hook-form": "^7.39.7",
    "react-modal": "^3.16.1",
    "react-spinners": "^0.13.6",
    "reactjs-popup": "^2.0.5",
    "styled-components": ">=5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@size-limit/preset-small-lib": "^5.0.5",
    "@storybook/addon-docs": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addons": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/theming": "^6.5.15",
    "@types/enzyme": "^3.10.9",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/react-modal": "^3.13.1",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.15",
    "@types/styled-system": "^5.1.13",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-jest": "^27.2.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4",
    "eslint-plugin-storybook": "^0.6.8",
    "husky": "4.3.8",
    "inquirer-directory": "^2.2.0",
    "jest": "^27.2.5",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^11.2.0",
    "plop": "^2.7.4",
    "polished": "^4.1.3",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "size-limit": "^5.0.5",
    "styled-components": "^5.3.1",
    "styled-system": "^5.1.5",
    "ts-jest": "^27.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@emoji-mart/data": "^1.0.8",
    "@emoji-mart/react": "^1.0.1",
    "@hookform/resolvers": "^2.9.10",
    "caniuse-lite": "^1.0.30001434",
    "flatlist-react": "^1.5.6",
    "linkify-react": "^4.0.2",
    "linkifyjs": "^4.0.2",
    "react-hook-form": "^7.39.7",
    "react-modal": "^3.16.1",
    "react-select": "^5.7.0",
    "react-spinners": "^0.13.6",
    "reactjs-popup": "^2.0.5",
    "tinycolor2": "^1.4.2",
    "yup": "^0.32.11"
  }
}

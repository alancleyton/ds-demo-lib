  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/ds-demo-lib.esm.js",
  "jsnext:main": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/alancleyton/ds-demo-lib.git"
  },
  "scripts": {
    "start": "yarn storybook",
    "build": "rollup -c",
    "test": "jest --passWithNoTests",
    "lint": "eslint src -c .eslintrc.js --ext \"ts,tsx\"",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "clean:tags": "git tag -l | xargs git tag -d && git fetch --tags",
    "prebuild": "rimraf dist",
    "pretest": "jest --clearCache",
    "prettify": "prettier --check src --write",
    "test:changed": "jest -o",
    "test:ci": "jest --ci --maxWorkers=2 --collectCoverage",
    "test:coverage": "jest --coverage --colors",
    "test:staged": "jest --ci --findRelatedTests",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "yarn prettify",
      "yarn lint --fix",
      "yarn test:staged"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
